@model IEnumerable<MVCGarage.Models.CheckIn>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParkingSpot.Label)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vehicle.Owner)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CheckInTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Booked)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Free)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ParkingSpot.Label)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.Owner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckInTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Booked)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Free)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
